{
    "name": "ordinary9843/ghostscript",
    "description": "Use ghostscript guess and convert PDF file version for PHP.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jerry Chen",
            "email": "ordinary9843@gmail.com"
        }
    ],
    "require": {   
        "php": ">=7.1.33"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5||^6.0||^7.0",
        "phpdocumentor/phpdocumentor": "^2.9",
        "mockery/mockery": "^1.0"
    },
    "autoload": {  
        "psr-4": { 
            "Ordinary9843\\": "src/"   
        }  
    },
    "autoload-dev" : {
        "psr-4" : {
            "Ordinary9843\\" : "tests/"
        }
    },
    "scripts" : {
        "test" : "./vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    }
}
