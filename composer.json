{
    "name": "ordinary9843/ghostscript",
    "description": "Use ghostscript to merge all PDF files or guess and convert PDF file version",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jerry Chen",
            "email": "ordinary9843@gmail.com"
        }
    ],
    "require": {   
        "php": ">7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {  
        "psr-4": { 
            "Ordinary9843\\": "src/"   
        }  
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit --coverage-clover ./coverage.xml --coverage-html ./report"
    }
}
